def gitCloneFrom(branch,credentialsId,url ){
    try{
        git branch: &quot;${branch}&quot;, credentialsId: &quot;${credentialsId}&quot;, url: &quot;${url}&quot;
    } catch(Exception e) {
        println(&quot;Exception: ${e}&quot;)
    }
}</script>
